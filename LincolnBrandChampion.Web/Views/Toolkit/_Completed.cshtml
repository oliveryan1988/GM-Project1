<script type="text/javascript">
    $(document).ready(function () {  //On document ready
        $('#step1ppt').click(function (e) {  //For each a (anchor), set the following function

            // var file = 'Step1PPT.pptx';
            ga('send', 'event', 'button', 'click', 'Checkpoint1SelfStudyPPTX.pptx');
            $.ajax({
                type: 'POST',
                url: '@Url.Action("Clicks", "Toolkit")',
                data: { file: this.href },
                dataType: 'json',
                success: function (json) {
                    // _gaq.push(['_trackEvent', 'File', 'downloads', 'Checkpoint1SelfStudyPPTX.pptx']);
                    var link = $("a", this);
                    if (link.length != 0)
                        window.location = link.attr("href");

                }

            });
        });
