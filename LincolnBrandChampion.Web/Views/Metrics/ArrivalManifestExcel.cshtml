@using LincolnBrandChampion.Model.Models
@using LincolnBrandChampion.Web.Helpers
@model List<RegistrationReportModel>
@{
    Layout = null;
    Response.ContentType = "application/vnd.ms-excel";
    Response.AddHeader("Content-Disposition", "attachment; filename=ArrivalManifest.xls");
}

<?xml version="1.0" encoding="utf-16"?>
<ss:Workbook xmlns:ss="urn:schemas-microsoft-com:office:spreadsheet">
    <ss:Worksheet ss:Name="Sheet1">
        <ss:Table>
            <ss:Row>
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Last Name</ss:Data></ss:Cell>
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">First Name</ss:Data></ss:Cell>
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Market</ss:Data></ss:Cell>
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Mobile phone #</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Dealership Phone #</ss:Data></ss:Cell> 	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Email Address</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Method of Transportation</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Arrival Date</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Arrival Time</ss:Data></ss:Cell> 	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Airline</ss:Data></ss:Cell> 	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Flight Number</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Arriving From (Departure City)</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">If this is a connection, please provide your originating flight information</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Confirm# for ground trans</ss:Data></ss:Cell>
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Transportation Requirements</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Dealership Name</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Title</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Department</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">P&A Code</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">STARs ID</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">City</ss:Data></ss:Cell> 	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">State</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">ZIP</ss:Data></ss:Cell>
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Zone</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Registration Date</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Edited by Guest on (Date)</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Badge Name</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Admin Notes</ss:Data></ss:Cell>	
                    <ss:Cell ecProperty=""><ss:Data ss:Type="String">Attendance Confirmed</ss:Data></ss:Cell>
            </ss:Row>
            @foreach (var item in Model)
            {
                <ss:Row>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatToCaps(item.LAST_NAME)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatToCaps(item.FIRST_NAME)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatTrim(item.MARKET)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatPhoneNumber(item.PHONE)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatPhoneNumber(item.DLR_PHONE)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatEmail(item.EMAIL_ID)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatToCaps(item.TRANSPORT_METHOD)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatDate(item.ARR_DATE)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatTime(item.ARR_TIME)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatToCaps(item.ARR_AIRLINE)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatToCaps(item.ARR_FLIGHT_NUM)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatToCaps(item.ARR_DEP_CITY)</ss:Data></ss:Cell> 
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatTrim(item.CONNECTING_FLIGHT_NOTES)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatTrim(item.CAR_CONFIRM)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatTrim(item.TRANSPORTATION_NEED)/ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatToProper(item.DLR_NAME)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatToProper(item.TITLE)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatToProper(item.DEPT_INFO)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatToCaps(item.PA_CODE)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatToCaps(item.STARS_ID)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatToProper(item.DLR_CITY)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatState(item.DLR_STATE)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatToProper(item.DLR_ZIP)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatToProper(item.ZONE)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatDate(item.CREATED_DATE)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatDate(item.UPDATE_DATE)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatBadgeName(item.BADGE_NAME)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatTrim(item.ADMIN_NOTES)</ss:Data></ss:Cell>
                    <ss:Cell><ss:Data ss:Type="String">@ReportFormatHelper.FormatAttended(item.ATTENDED)</ss:Data></ss:Cell>
                </ss:Row>
            }
        </ss:Table>
    </ss:Worksheet>
</ss:Workbook>